<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DRL_ContactChangeValidation</fullName>
    <active>true</active>
    <errorConditionFormula>AND( NOT( ISNEW() ) ,  NOT(ISBLANK(PRIORVALUE( Contact__c )) ) ,  ISCHANGED( Contact__c ) )</errorConditionFormula>
    <errorDisplayField>Contact__c</errorDisplayField>
    <errorMessage>Contact cannot be updated!</errorMessage>
</ValidationRule>
